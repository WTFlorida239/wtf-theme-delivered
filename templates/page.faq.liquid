  }

  .faq-section {
    max-width: 900px;
    margin: 0 auto 4rem;
    padding: 0 2rem;
  }

  .faq-section__heading {
    font-size: 2rem;
    font-weight: bold;
    margin-bottom: 2rem;
    color: #ff6b35;
    border-bottom: 3px solid #ff6b35;
    padding-bottom: 0.5rem;
  }

  .faq-item {
    background: #f8f9fa;
    padding: 2rem;
    margin-bottom: 1.5rem;
    border-radius: 8px;
    border-left: 4px solid #ff6b35;
  }

  .faq-question {
    font-size: 1.4rem;
    font-weight: bold;
    margin-bottom: 1rem;
    color: #333;
  }

  .faq-answer {
    font-size: 1.1rem;
    line-height: 1.7;
    color: #555;
  }

  .faq-answer p {
    margin-bottom: 1rem;
  }

  .faq-answer a {
    color: #ff6b35;
    text-decoration: none;
  }

  .faq-answer a:hover {
    text-decoration: underline;
  }

  .faq-contact {
    font-size: 1.2rem;
    line-height: 1.8;
  }

  @media (max-width: 768px) {
    .faq-hero__title {
      font-size: 2rem;
    }

